create database demo_tpc;
use demo_tpc;

create table emp_store(
 employeeId INT NOT NULL,
 name VARCHAR(45) NOT NULL,
 salary INT NOT NULL,
 PRIMARY KEY (employeeId)
);

create table mgr (
departmentName varchar(50) not null,
 employeeId INT NOT NULL 
);


select * from emp_store;
select * from mgr;
